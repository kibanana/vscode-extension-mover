{
	"name": "extension-mover",
	"displayName": "Extension Mover",
	"description": "It helps to move extensions to another workspace for Visual Studio Code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
    "onCommand:extension.exporter",
    "onCommand:extension.importer"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.exporter",
        "title": "Extension Mover: export extensions"
      },
      {
				"command": "extension.importer",
        "title": "Extension Mover: import extensions"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@babel/cli": "^7.7.7",
		"@babel/core": "^7.7.7",
		"@babel/preset-env": "^7.7.7",
		"@types/child-process-promise": "^2.2.1",
		"@types/copy-paste": "^1.1.30",
		"@types/graceful-fs": "^4.1.3",
		"child-process-promise": "^2.2.1",
		"child_process": "^1.0.2",
		"childprocess": "^2.0.2",
		"copy-paste": "^1.3.0",
		"graceful-fs": "^4.2.3",
		"os": "^0.1.1",
		"path": "^0.12.7"
	}
}

{
	"name": "vscode-extension-mover",
	"displayName": "VSCode Extension Mover",
	"description": "It helps to move vscode extensions to another workspace",
	"version": "1.0.0",
	"publisher": "yewon63",
	"engines": {
		"vscode": "1.52.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.exporter",
		"onCommand:extension.deletingHelper"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.exporter",
				"title": "Extension Mover: export all extensions (save as file or save to clipboard)"
			},
			{
				"command": "extension.deletingHelper",
				"title": "Extension Mover: help deleting all extensions"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/woni-d/vscode-extension-mover.git"
	},
	"devDependencies": {
		"@babel/cli": "^7.12.13",
		"@babel/core": "^7.12.13",
		"@babel/preset-env": "^7.12.13",
		"@types/mocha": "^5.2.6",
		"@types/node": "^14.14.25",
		"mocha": "^6.1.4",
		"ts-node": "^9.1.1",
		"tslint": "^5.12.1",
		"typescript": "^4.1.3",
		"vscode": "^1.1.37",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@types/child-process-promise": "^2.2.1",
		"@types/graceful-fs": "^4.1.3",
		"@types/vscode": "^1.53.0",
		"child-process-promise": "^2.2.1",
		"graceful-fs": "^4.2.3",
		"open": "^7.4.0",
		"os": "^0.1.1",
		"os-name": "^3.1.0",
		"path": "^0.12.7",
		"util": "^0.12.3"
	}
}

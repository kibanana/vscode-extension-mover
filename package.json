{
	"name": "extension-mover",
	"displayName": "Extension Mover",
	"description": "It helps to move extensions to another workspace in Visual Studio Code",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.exporter"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.exporter",
				"title": "Extension Mover: export extensions"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@babel/cli": "^7.12.13",
		"@babel/core": "^7.12.13",
		"@babel/preset-env": "^7.12.13",
		"@types/mocha": "^5.2.6",
		"@types/vscode": "^1.41.0",
		"mocha": "^6.1.4",
		"ts-node": "^9.1.1",
		"tslint": "^5.12.1",
		"typescript": "^4.1.3",
		"vscode": "^1.1.37",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@types/child-process-promise": "^2.2.1",
		"@types/graceful-fs": "^4.1.3",
		"child-process-promise": "^2.2.1",
		"graceful-fs": "^4.2.3",
		"os": "^0.1.1",
		"os-name": "^3.1.0",
		"path": "^0.12.7"
	}
}

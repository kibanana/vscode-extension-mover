{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"lib": ["es6"],
		"outDir": "out",
		"rootDir": "src",
		"typeRoots" : [
			"node",
			"vscode"
		],
		"moduleResolution": "node",
		"sourceMap": true,
		"strict": true,
		"esModuleInterop": true
	},
	"exclude": [
		"node_modules",
		".vscode-test",
		"out"
	]
}
